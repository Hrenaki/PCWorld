@if (User.Identity == null || !User.Identity.IsAuthenticated)
{
	<partial name="~/Views/Account/LoginPartial.cshtml" />

	<script>
		$(document).ready(function () {
			 setLoginHandler('@ViewData["modal"]');
			 setRegisterHandler('@ViewData["modal"]');
		});
	</script>
}
else
{
	<partial name="~/Views/Account/AccountPartial.cshtml" />

	<script>
		$(document).ready(function () {
			 setLogoutHandler('@ViewData["modal"]');
		});
	</script>
}